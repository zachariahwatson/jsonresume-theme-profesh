{{#if resume.education.length}}
<section id="education">
    <h2>Education</h2>
    {{#each resume.education}}
    {{#> item title=institution subtitle=studyType location=city startDate=startDate endDate=endDate}}
    {{#if score}}
    <ul>
        <li>
            <div class="score">
                {{score}}
            </div>
        </li>
        {{#if score2}}
        <li>
            <div class="score2">
                {{score2}}
            </div>
        </li>
        {{/if}}
    </ul>
    {{/if}}
    {{#if courses.length}}
    <ul class="courses">
        {{#each courses}}
        <li>{{.}}</li>
        {{/each}}
    </ul>
    {{/if}}
    {{/item}}
    {{/each}}
</section>
{{/if}}
